{
    "name": "webpack-builder",
    "private": true,
    "scripts": {
        "build": "lerna run build",
        "test": "lerna run test",
        "bootstrap": "lerna bootstrap",
        "pack": "lerna exec -- npm pack",
        "version-bump:canary": "lerna version prerelease --exact --no-push --no-git-tag-version --no-commit-hooks -y",
        "pack:canary": "run-s version-bump:canary pack"
    },
    "devDependencies": {
        "lerna": "3.16.4",
        "npm-run-all": "4.1.5"
    }
}
