{
  "name": "@reactway/image-loader",
  "version": "1.0.0-alpha.15",
  "description": "A plugin made for webpack to load images, optimize and resize.",
  "keywords": [
    "webpack",
    "webpack-builder",
    "reactway",
    "plugin",
    "images",
    "images-loader"
  ],
  "authors": [
    "Martynas Žilinskas <martynas@quatrodev.com> (https://github.com/MartynasZilinskas)",
    "Dovydas Navickas <dovydas@quatrodev.com> (https://github.com/DovydasNavickas)",
    "Eimantas Dumšė <eimantas@quatrodev.com> (https://github.com/EimantasDumse)"
  ],
  "repository": "github:reactway/webpack-builder",
  "bugs": "https://github.com/reactway/webpack-builder/issues",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "pretest": "tsc -p . --noEmit && tslint -p .",
    "test": "jest"
  },
  "files": [
    "dist",
    "**/*.md"
  ],
  "dependencies": {
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.0.0",
    "mime": "^2.4.2",
    "schema-utils": "^1.0.0",
    "debug": "^4.1.1",
    "sharp": "^0.22.1",
    "chalk": "^2.4.2",
    "loader-utils": "^1.2.3",
    "fs-extra": "^8.0.1",
    "tslib": "^1.9.3",
    "upath": "^1.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/imagemin": "^6.0.0",
    "@types/imagemin-gifsicle": "^5.2.0",
    "@types/imagemin-optipng": "^5.2.0",
    "@types/imagemin-pngquant": "^7.0.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@types/mime": "^2.0.1",
    "@types/sharp": "^0.22.2",
    "@types/loader-utils": "^1.1.3",
    "@types/node": "^12.0.2",
    "@types/debug": "^4.1.4",
    "webpack": "^4.31.0",
    "@types/webpack": "^4.4.31",
    "istanbul-azure-reporter": "^0.1.4",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "simplr-tslint": "^1.0.0-alpha.14",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.3.3333",
    "@types/jest": "^24.0.11",
    "memory-fs": "^0.4.1",
    "@types/memory-fs": "^0.3.2"
  },
  "peerDependencies": {},
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "testRegex": "/__tests__/.*\\.(test|spec).(ts|tsx)$",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/__tests__/*",
      "!src/index.ts",
      "!src/**/__tests__/compiler/compiler.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "istanbul-azure-reporter"
    ]
  }
}
